# Your Medusa backend, should be updated to where you are hosting your server. Remember to update CORS settings for your server. See – https://docs.medusajs.com/usage/configurations#admin_cors-and-store_cors
MEDUSA_BACKEND_URL=http://localhost:9000
# Optional per-environment backend URLs (populate NEXT_PUBLIC_MEDUSA_BACKEND_URL automatically)
MEDUSA_DEV_BACKEND_URL=http://localhost:9000
MEDUSA_PROD_BACKEND_URL=
MEDUSA_ADMIN_TOKEN=

# Your publishable key that can be attached to sales channels. See - https://docs.medusajs.com/development/publishable-api-keys
NEXT_PUBLIC_MEDUSA_PUBLISHABLE_KEY=
# Optional per-environment keys (used to populate NEXT_PUBLIC_MEDUSA_PUBLISHABLE_KEY)
MEDUSA_DEV_PUBLISHABLE_KEY=
MEDUSA_PROD_PUBLISHABLE_KEY=
NEXT_PUBLIC_MEDUSA_BACKEND_URL=
# Optional strict guard: set to 1 to block localhost backend URLs whenever NODE_ENV=production
ENFORCE_PROD_BACKEND_URL_GUARD=0
# Optional SDK request logging. Set to 1 only when debugging request payloads/headers locally.
MEDUSA_SDK_DEBUG=0

# Your store URL, should be updated to where you are hosting your storefront.
NEXT_PUBLIC_BASE_URL=http://localhost:8000

# Your preferred default region. When middleware cannot determine the user region from the "x-vercel-country" header, the default region will be used. ISO-2 lowercase format. 
NEXT_PUBLIC_DEFAULT_REGION=us

# Optional: explicitly set the storefront's sales channel (helps expose inventory quantities)
# Example: NEXT_PUBLIC_DEFAULT_SALES_CHANNEL_ID=sc_01ABCDEF...
NEXT_PUBLIC_DEFAULT_SALES_CHANNEL_ID=

NEXT_PUBLIC_AUTHNET_ENV=sandbox

# Your Next.js revalidation secret. See – https://nextjs.org/docs/app/building-your-application/data-fetching/fetching-caching-and-revalidating#on-demand-revalidation
REVALIDATE_SECRET=
NEXT_PUBLIC_SANITY_PROJECT_ID=
NEXT_PUBLIC_SANITY_DATASET= #development || production

# Authorize.Net keys
NEXT_PUBLIC_CLIENT_KEY=
NEXT_PUBLIC_API_LOGIN_ID=

# Maintenance Mode - 0 for off / 1 for on - List urls to allow in maintenance mode
MAINTENANCE_MODE="0"
ALLOWED_PATHS=

CONTEXT7_API_KEY=
